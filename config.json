{
  "locals": {
    "url": "http://www.luckyrandom.com",
    "name": "The Luckyrandom's blog",
    "owner": "Chenliang Xu",
    "description": "I enjoy thinking; I know statistics; I can code."
  },
  "plugins": [
    "./node_modules/wintersmith-coffee/plugin.coffee",
    "wintersmith-nunjucks",
    "./plugins/contentHelpers.js",
    "wintersmith-asis",
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-pandoc/",
    "wintersmith-tag",
    "./plugins/tag_feed.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "urlPackage": "url",
    "typogr": "typogr"
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "template": "index.html",
    "perPage": 5
  },
  "ignore": [
    "articles/_draft_*/**",
    "articles/*.mk",
    "articles/*/Makefile",
    "**/config.rb",
    "**/sass",
    "**/.sass-cache",
    "**/.DS_Store",
    "**/*.Rmd",
    "**/.gitignore",
    "s3RoutingRules.xml",
    "articles/*/cache/**",
    "articles/*/noshare/**",
    "**/.#**"
  ],
  "baseUrl": "/blog/"
}
